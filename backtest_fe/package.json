  "description": "Professional backtesting dashboard built with React, TypeScript, Tailwind CSS, and Recharts for visualizing trading strategy performance",
  "private": true,
  "type": "module",
  "keywords": [
    "backtesting",
    "trading",
    "finance",
    "react",
    "typescript",
    "charts",
    "dashboard",
    "tailwindcss",
    "react-icons",
    "recharts",
    "technical-analysis"
  ],
  "author": "Backtesting Team",
  "license": "MIT",
  "homepage": ".",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-repo/backtesting-frontend.git"
  },
  "bugs": {
    "url": "https://github.com/your-repo/backtesting-frontend/issues"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
